tp   : type.
nat  : tp.
bool : tp.

exp  : tp -> type.

tt   : exp bool.
ff   : exp bool.

z    : exp nat.
suc  : exp nat -> exp nat.
eq   : exp nat -> exp nat -> exp bool.

if   : exp bool -> exp T -> exp T -> exp T.
letv : exp T1 -> (exp T1 -> exp T2) -> exp T2.

eval : exp T -> exp T -> type.

ev_z: eval z z.