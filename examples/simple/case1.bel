nat: type.
z   : nat.
suc : nat -> nat.

schema w = block nat;

rec copynat1 : {g:(w)*} (nat)[g] -> (nat)[g] = 
    FN g => fn e => copynat1 [g] e;


rec copynat : {g:(w)*} (nat)[g] -> (nat)[g] =
  FN g =>
    fn e =>
      case e of               box (g. z)              : nat[g] => box(g. z)
             | {U :: nat [g]} box (g. suc (U![id]!)) : nat[g] => copynat [g] box(g. U![id]!)
;
