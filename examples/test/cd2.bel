nat : type.
z   : nat.
suc : nat -> nat.

list : nat -> type.
cons : list N -> list (suc N).
nil : list z.

rec conslist : {G'::nat[]} (list (suc G'))[] -> nat[] = mlam G => fn e =>
  case e of
  | [] cons T          => [] z
;

%not
rec badlist : {G'::nat[]} (list (suc G'))[] -> nat[] = mlam G => fn e =>
  case e of 
  | [] nil          => [] z
;
