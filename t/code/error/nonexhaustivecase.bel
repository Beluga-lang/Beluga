nat  : type.
z    : nat.
s    : nat -> nat.
cmp  : type.
lt   : cmp.
eq   : cmp.
gt   : cmp.

rec compare : [ |- nat] -> [ |- nat] -> [ |- cmp] =
fn x => fn y => case x of
| [ |- z] => case y of
    | [ |- z] => [ |- eq]
    | [ |- _] => [ |- lt]
| [ |- s N] => case y of
    | [ |- z] => [ |- gt]
    | [ |- s M] => compare [ |- N] [ |- M]
;

rec max : [ |- nat] -> [ |- nat] -> [ |- nat] =
fn x => fn y => let c = compare x y in
    case c of
    | [ |- lt] => y
    | [ |- gt] => x
    | [ |- _] => x
;

let y = max [ |- (s (s (s z)))] [ |- (s z)] ;
