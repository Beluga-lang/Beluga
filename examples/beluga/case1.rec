nat: type.
z   : nat.
suc : nat -> nat.

schema w = some [ ] block nat;

rec copynat1 : {g:(w)*} (nat)[g] -> (nat)[g] = 
    FN g => fn e => copynat1 [g] e;


rec copynat : {g:(w)*} (nat)[g] -> (nat)[g] =
  FN g =>
    fn e =>
      case e of box (g. z)             => box(g. z)
              | box (g. suc (U![id]!)) => copynat [g] box(g. U![id]!)
;
