datatype A: ctype #positivity
=
 | A2: A * A -> A
;

datatype B: ctype 
=
 | B2: ((A -> A) -> B) -> B
 | B3: A -> (A * B) -> B 
 | B4: (A -> (A -> B)) -> B
;

datatype C: ctype #positivity
=
 | C1: (A -> B) * (A -> C) -> C
 | C2: (B -> A) -> (C * (A -> C)) -> C
 | C3: ((A -> A) -> (B -> C)) * (A -> (B * C)) -> C
 | C5: (A -> B) -> (B * C) -> A -> C

;

