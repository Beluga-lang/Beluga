(* This example will not typecheck until you implement subtyping *)
let
  fun dunno : int -> int & int -> real x = 55
    (* OK because 55 synthesizes int, which is a subtype of real *)
  
  val y = dunno 0
  val z = y * y
  val z' = y * y
  val z'' = (y : real) * (y : real)
in
  (dunno, y, z, z', z'')
end;
