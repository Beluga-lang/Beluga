nat :  type.

z : nat.
s : nat -> nat.

rec plus : [ |- nat] -> [ |- nat] -> [ |- nat] =
fn x => fn y =>  case x of
| [ |- z]  => y
| [ |- s U]  =>
  let [ |- V] = plus [ |- U ] y in
    [ |- s V]
;
