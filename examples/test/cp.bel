term :  type.
z    : term.
suc  : term -> term.


schema termCtx = some [] term;


rec fff : {g:(termCtx)*} term[g] -> term[g] =
FN g => fn e =>
  case e of
  | [g]  z     => [g] z

  | [g]  suc (U ..)   => [g] z

  | [g]  #p ..   => [g] z
;



rec fffForgotParamVarCase : {g:(termCtx)*} term[g] -> term[g] =
FN g => fn e =>
  case e of %not
  | [g]  z     => [g] z

  | [g]  suc (U ..)   => [g] z

%%%  | [g]  #p ..   => [g] z
;
