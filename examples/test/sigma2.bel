tp : type.
unit : tp.
arr : tp -> tp -> tp.

exp : tp -> type.
unitexp : exp unit.

schema assn = block t:tp. exp t;

%not
rec fff : {g:(assn)*} tp[] -> tp[]
= FN g => fn e => fff[g, zz:block u:tp. exp unit] e;

rec ggg : {g:(assn)*} tp[] -> tp[]
= FN g => fn e => ggg[g, zz:block u:tp. exp u] e;

rec hhh : {g:(assn)*} tp[] -> tp[]
= FN g => fn e => hhh[g, zz:block u:tp. exp u, zz1:block u':tp. exp u'] e;

%not
rec iii : {g:(assn)*} tp[] -> tp[]
 = FN g => fn e => iii[g, zz:block u:tp. exp u, zz1:block u':tp, u':tp. exp u'] e;
