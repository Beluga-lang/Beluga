(* This won't typecheck until you implement subtyping *)

let
  fun plus : (int -> int -> int) & (real -> real -> real) x =
      fn y => x + y
  
  val tuple = 
      (3 + 5.0,
       33.0 + 55.0,
       3.0 + 3,
       10 + 5,
       (10 + 5) : real)
  
  val tuple' = 
       ( plus 3 5.0,
        plus 33.0 55.0,
        plus 3.0 3,
        plus 10 5,
        (plus 10 5) : real)
in
  (tuple, tuple')
end;
