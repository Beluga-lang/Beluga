%{
  CH 3:  Untyped arithmetic expressions
  Author: Brigitte Pientka

  SYNTAX
}%


datatype term  : type = 
| true  : term
| false : term
| switch: term -> term -> term -> term
;

% Examples : Valid Terms

let v1 = [ |- switch false true (switch true false true)] ;


%not
let w2 = [ |- switch true false] ;


