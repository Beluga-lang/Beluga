tp : type.
nat : tp.

term : tp -> type.
z    : term nat.
suc  : term nat -> term nat.


schema termCtx = some [t:tp] block galaga:term t, gargravarr:term t, _t:term t;


rec fff : {g:termCtx} [g |- term nat] -> [g |- term nat] =
mlam g => fn e =>
  case e of
  | [g |- z]     => [g |- z]
  | [g |- suc (U[..])]   => [g |- z]
  | [g |- #p.1[..]]   => [g |- z]
  | [g |- #p.3[..]]   => [g |- z]
  | [g |- #p.2[..]]   => [g |- z]
;


rec fffRedundantWrongProjCases : {g:termCtx} [g |- term nat] -> [g |- term nat] =
mlam g => fn e =>
  case e of --not
  | [g |- z]     => [g |- z]
  | [g |- suc (U[..])]   => [g |- z]
  | [g |- #p.2[..]]   => [g |- z]
  | [g |- #p.1[..]]   => [g |- z]
  | [g |- #p.2[..]]   => [g |- z]
;



rec fffRedundantWrongProjCases' : {g:termCtx} [g |- term nat] -> [g |- term nat] =
mlam g => fn e =>
  case e of --not
  | [g |- z]     => [g |- z]
  | [g |- #p.1[..]]   => [g |- z]
  | [g |- suc (U[..])]   => [g |- z]
  | [g |- #p.1[..]]   => [g |- z]
  | [g |- #p.3[..]]   => [g |- z]
  | [g |- #p.3[..]]   => [g |- z]
;




