%theorem m-subst : forall* {M: term -> term}{M': term -> term}
		          {N: term}{N':term}
		  forall  {D1: {x:term} x => x -> M x => M' x}
		          {D2: N => N'}
		  exists  {E:  M N => M' N'}
        	  true.
%assert (m-subst _ _ _).



