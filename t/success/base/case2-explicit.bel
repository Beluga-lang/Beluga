nat: type.
z   : nat.
suc : nat -> nat.

irrelevancy : type.
irrelevant_con : irrelevancy.

schema w = nat;

rec copynat : {g:w} [g |- nat] -> [g |- nat] =
mlam g => fn e =>
 case e of --not
    [g |- z] : [g |- nat] => [g |- z]
  | {U:[g |- nat]}
     [g |- suc (U[..])] : [g |- nat] => copynat [g] [g |- (U[..])]

;
