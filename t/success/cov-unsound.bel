target:type.
z: target.
tlam: (target → target) → target.

schema tctx = target;

rec f: (h:tctx) [h,y:target |- target] → [h, y:target |- target] =
 fn e => case e of %not
   | [h,y:target |-  U .. ] => e ;

rec f0: (h:tctx) [h |- target] → [h |- target] =
    fn e ⇒ case e of
      |[h |- #p ..] ⇒ e
      |[h |- tlam (\x.U … x)] ⇒
       let [h,y:target |-  U'…] = f0 [h,y:target |-  U … y] in
       [h |- tlam \x.U'…]
;
