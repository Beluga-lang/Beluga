inductive A: ctype 
=
 | A2: A * A -> A
;

inductive B: ctype 
=
 | B2: ((A -> A) -> B) -> B
 | B3: A -> (A * B) -> B 
 | B4: (A -> (A -> B)) -> B
;

inductive C: ctype 
=
 | C1: (A -> B) * (A -> C) -> C
 | C2: (B -> A) -> (C * (A -> C)) -> C
 | C3: ((A -> A) -> (B -> C)) * (A -> (B * C)) -> C
 | C4:  ((A -> C) -> (B -> C)) * (A -> (B * C)) -> C % fail

;

