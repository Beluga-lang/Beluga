%%% Language of formulas for intuitionistic and classical
%%% predicate calculus.
%%% Author: Frank Pfenning

i : type.  % individuals
%name i T. % S % more than one currently disallowed

o : type.  % formulas
%name o A. % B C % more than one currently disallowed

&      : o -> o -> o.
imp    : o -> o -> o.
or     : o -> o -> o.
not    : o -> o.
true   : o.
false  : o.
forall : (i -> o) -> o.
exists : (i -> o) -> o.

