%{
  CH 3:  Untyped arithmetic expressions
  Author: Brigitte Pientka

  SYNTAX
}%


datatype term  : type = 
| true  : term
| false : term
| if_then_else : term -> term -> term -> term
;

% Examples : Valid Terms

let v1 = [ |- if_then_else false true (if_then_else true false true)] ;


%not
let w2 = [ |- if_then_else true false] ;


