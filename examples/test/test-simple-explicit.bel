nat : type.
z : nat.
s : nat -> nat.

schema w = some [] nat;

rec id-test : {g:(w)*} nat [g] -> nat [g] =
    FN g => fn x => x ;



rec identity1 : {g:(w)*} nat [g] -> nat [g] =
  FN g =>
    fn x =>
      case x of
          [g] z   : nat [g] => [g] z
        | {U:: nat [g]} [g] s (U .. ) : nat [g] => [g] s (U .. );



rec identity2 : {g:(w)*} nat [g] -> nat [g] =
  FN g =>
    fn x =>
      case x of
          [g] z   : nat [g] => x
        | {U::nat[g]} [g] s (U .. ) : nat [g] => [g] s (U .. );


rec identity2 : {g:(w)*} nat [g] -> nat [g] =
  FN g =>
    fn x =>
      case x of
          [g] z   : nat [g] => x
        | {U::nat[g]} [g] s (U .. ) : nat [g] => x;





