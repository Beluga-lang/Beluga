tm : type.
i : tm.

schema ctx = tm;

rec f : {g:ctx}{h:ctx} {$S :[h |- # g]} { M : [g |- tm]} [h |- tm] =
mlam g => mlam h => mlam $S => mlam M => [h |- M[$S[..]] ]
;

rec ff : {g:ctx}{h:ctx} {$S : [h |- g]} { M : [g |- tm]} [h |- tm] =
mlam g => mlam h => mlam $S => mlam M => f [g][h][h |- $S[..]] [g |- M]
;
