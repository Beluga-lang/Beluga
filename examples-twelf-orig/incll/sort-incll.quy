% l1 = (cons (s s s z) (cons (s z) (cons (s s z) nil))).
% l2 = (cons (s s s z) (cons (s s z) (cons (s z) (cons (s s z) nil)))).

l1 = (s s s z) | (s z) | (s s z) | nil.
l2 = (s s s z) | (s s z) | (s z) | (s s z) | nil.

%query 1 * (sqnt rnil rnil rnil rnil (^ (mergeSort l1 L))).
%query 1 * (sqnt rnil rnil rnil rnil (^ (mergeSort l2 L))).

