LF tp : type =
  | base : tp
  | arr : tp → tp → tp
;

LF eq : tp → tp → type =
  | eq_base : eq base base
  | eq_arr : eq A1 A2 → eq B1 B2 → eq (arr A1 B1) (arr A2 B2)
;

proof tp-refl : {A : ⊢ tp} [ ⊢ eq A A ] =
/ total 1 /
intros
{ A : ( |- tp)
|
; split [ |- A] as
  case arr:
  { X : ( |- tp), X1 : ( |- tp)
  |
  ; ?
  }
  case base:
  {
  |
  ; solve [ |- eq_base]
  }
}
;
